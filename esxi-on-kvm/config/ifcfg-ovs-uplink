DEVICE=ovs-uplink
DEVICETYPE=ovs
TYPE=OVSIntPort
BOOTPROTO=static
ONBOOT=yes
IPADDR=ETH0IPPLACEHOLDER
NETMASK=ETH0NETMASKPLACEHOLDER
OVS_BRIDGE=ovs-br0
NM_CONTROLLED=no
EC2SYNC=no
MACADDR=ETH0MACPLACEHOLDER
GATEWAY=ETH0GATEWAYPLACEHOLDER
OVS_EXTRA="set Interface eth0 other-config:hwaddr=$MACADDR"
ETHTOOL_OPTS="-K ${DEVICE} gso off; -K ${DEVICE} gro off; -K ${DEVICE} tso off; -K ${DEVICE} txvlan off; -K ${DEVICE} sg off; -K ${DEVICE} ufo off; -K ${DEVICE} lro off"
MTU=9000



